{
    "expo": {
        "name": "testproject",
        "slug": "testproject",
        "scheme": "testproject",
        "version": "1.0.0",
        "runtimeVersion": "50.0.0",
        "sdkVersion": "50.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "image": "latest",
            "supportsTablet": true,
            "bundleIdentifier": "com.vladiusftw.testproject"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.vladiusftw.testproject"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "extra": {
            "router": {
                "origin": false
            }
        }
    }
}
